/* pages/consumer/reportForSpend/reportForSpend.wxss */
.tips {
  margin: 60rpx 30rpx 0 30rpx;
}
.weui-form__control-area {
  margin-top: 30rpx;
}
.dataListBox {
  padding: 0 30rpx;
}
.list {
  border-bottom: 1rpx solid #efefef;
  padding: 30rpx;
  position: relative;
}
.list::after {
  content: ">";
  font-family: '宋体';
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  right: 30rpx;
  top: 88rpx;
}
.list text{
  display: block;
  line-height: 60rpx;
}
.inputbox {
  height: 100rpx;
}
.btn {
  margin-top: -30rpx;
  margin-bottom: 50rpx;
  background-color: #fff;
  font-weight: normal;
  border: 1rpx solid #333;
}


.title {
  font-size: 42rpx;
  font-weight: bold;
  text-align: center;
  margin: 40rpx 0;
}
.box {
  margin: 0 60rpx;
  margin-bottom: 50rpx;
}
.box > view {
  padding: 30rpx 30rpx;
  border: 1rpx solid #aaa;
  margin-top: 60rpx;
  font-size: 36rpx;
  color: #454545;
  border-radius: 15rpx;
}
.all {
  text-align: center;
  font-size: 40rpx;
  margin-bottom: 200rpx;
}
.name {
  margin-top: 10rpx;
  text-align: right;
}