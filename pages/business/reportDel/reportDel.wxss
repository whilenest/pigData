/* pages/business/reportDel/reportDel.wxss */
.tips {
  margin: 60rpx 30rpx 60rpx 30rpx;
}
button {
  display: block;
  height: 100rpx;
  line-height: 95rpx;
  font-size: 38rpx;
  margin: 30rpx;
  padding: 0;
  background-color: #fff;
  font-weight: normal;
  border: 1rpx solid #333;
  margin-top: 130rpx;
  /* font-weight: normal; */
}
.reportBox {
  margin-top: 50rpx;
}
.reportBox .box {
  margin: 0 60rpx;
  border: 1rpx solid #aaa;
  margin-bottom: 50rpx;
}
.reportBox .box > view {
  padding: 7rpx 20rpx;
  border-bottom: 1rpx solid #aaa;
  font-size: 36rpx;
  color: #454545;
}

.box view.last {
  border: none;
  padding-top: 30rpx;
  padding-bottom: 30rpx;
}